<Window x:Class="WPF_Var1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Var1"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="500">
    <Grid>
        <StackPanel Width="250">
            <TextBlock Text="Введите двузначное число:" Margin="5,5,5,0"/>
            <TextBox x:Name="tbValue" Margin="5,5,5,0"/>
            <Button Content="Вычислить" Margin="5,5,5,0" Click="Button_Click"/>
            <TextBlock x:Name="tbResult" Margin="5,5,5,0"/>
        </StackPanel>
    </Grid>
</Window>
